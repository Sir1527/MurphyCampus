<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".controller.fragment.MyFragmentUpload"
    android:background="@drawable/background">

    <TextView
        android:id="@+id/title_user"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_marginTop="50dp"
        android:text="发布差事"
        android:textColor="#ff404042"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="315dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/order_bacground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_user"
        tools:ignore="MissingConstraints"></View>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="26dp"
        android:layout_height="19dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="28dp"
        android:text="姓名"
        android:textColor="#ff3d3d3d"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_user"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view2"
        android:layout_width="128dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="-10dp"
        android:background="@drawable/rounded_1"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7"></View>


    <EditText
        android:id="@+id/PublisherName"
        android:layout_width="119dp"
        android:layout_height="33dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/bg_cursor"
        android:hint="输入姓名"
        android:inputType="text"
        android:textColor="#ff333333"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginStart="128dp"
        android:text="差事悬赏"
        android:textColor="#ff3d3d3d"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7" />

    <View
        android:id="@+id/view3"
        android:layout_width="128dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="-10dp"
        android:background="@drawable/rounded_1"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9">
    </View>

    <EditText
        android:id="@+id/price"
        android:layout_width="119dp"
        android:layout_height="33dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/bg_cursor"
        android:hint="请输入价格"
        android:inputType="text"
        android:textColor="#ff333333"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toTopOf="@+id/view3" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="8dp"
        android:text="取件地址"
        android:textColor="#ff3d3d3d"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <View
        android:id="@+id/view4"
        android:layout_width="280dp"
        android:layout_height="50dp"
        android:layout_marginTop="42dp"
        android:background="@drawable/rounded_1"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/view2"></View>

    <EditText
        android:id="@+id/pickUpAddress"
        android:layout_width="270dp"
        android:layout_height="33dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/bg_cursor"
        android:hint="输入取件地址"
        android:inputType="text"
        android:textColor="#ff333333"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/view4"
        app:layout_constraintTop_toTopOf="@+id/view4" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:text="差事详情"
        android:textColor="#ff3d3d3d"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@+id/view4"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <View
        android:id="@+id/view5"
        android:layout_width="280dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="-10dp"
        android:background="@drawable/rounded_1"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11"></View>

    <EditText
        android:id="@+id/details"
        android:layout_width="270dp"
        android:layout_height="33dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/bg_cursor"
        android:hint="备注"
        android:inputType="text"
        android:textColor="#ff333333"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toTopOf="@+id/view5" />

    <View
        android:id="@+id/view7"
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/order_bacground"
        app:layout_constraintTop_toBottomOf="@+id/view6"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="24dp"></View>


    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:text="收件人姓名"
        android:textColor="#ff3d3d3d"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view11"
        android:layout_width="128dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="-10dp"
        android:background="@drawable/rounded_1"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8"></View>


    <EditText
        android:id="@+id/addressee"
        android:layout_width="119dp"
        android:layout_height="33dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/bg_cursor"
        android:hint="输入姓名"
        android:inputType="text"
        android:textColor="#ff333333"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/view11"
        app:layout_constraintTop_toTopOf="@+id/view11" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginStart="100dp"
        android:text="收件人电话"
        android:textColor="#ff3d3d3d"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <View
        android:id="@+id/view14"
        android:layout_width="128dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="-10dp"
        android:background="@drawable/rounded_1"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13">
    </View>

    <EditText
        android:id="@+id/addresseePhone"
        android:layout_width="119dp"
        android:layout_height="33dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/bg_cursor"
        android:hint="输入电话"
        android:inputType="text"
        android:textColor="#ff333333"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/view14"
        app:layout_constraintTop_toTopOf="@+id/view14" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="收货地址"
        android:textColor="#ff3d3d3d"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@+id/view11"
        app:layout_constraintTop_toBottomOf="@+id/view11" />

    <View
        android:id="@+id/view"
        android:layout_width="294dp"
        android:layout_height="50dp"
        android:layout_marginLeft="-10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/rounded_1"
        app:layout_constraintStart_toStartOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/textView19"></View>

    <EditText
        android:id="@+id/shippingAddress"
        android:layout_width="270dp"
        android:layout_height="33dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/bg_cursor"
        android:hint="地址"
        android:inputType="text"
        android:textColor="#ff333333"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <Button
        android:id="@+id/btn_upload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="发布"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toBottomOf="@+id/view7" />


</androidx.constraintlayout.widget.ConstraintLayout>